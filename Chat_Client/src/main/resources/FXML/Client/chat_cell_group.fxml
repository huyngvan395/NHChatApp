<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.chat_client.Controller.Client.ChatCellGroupController"
            prefHeight="80.0" prefWidth="335.0">
    <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="14.0" layoutY="15.0" pickOnBounds="true">
        <clip>
            <Circle centerX="25" centerY="25" radius="25"/>
        </clip>
        <Image url="@../../Image/userchatdefault.png"/>
    </ImageView>
    <Label layoutX="80.0" layoutY="15.0" text="Name">
        <font>
            <Font name="Arial Bold" size="14.0" />
        </font>
    </Label>
    <Label layoutX="80.0" layoutY="40.0" maxWidth="230.0" prefHeight="18.0" text="Last message">
        <font>
            <Font name="Arial" size="14.0" />
        </font>
    </Label>
</AnchorPane>
