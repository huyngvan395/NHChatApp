         <Font name="Arial" size="20.0" />
      </font>
      <graphic>
         <FontIcon iconLiteral="fa-lock" />
      </graphic>
   </Button>
   <Button fx:id="delete_acc" alignment="TOP_LEFT" layoutX="3.0" layoutY="498.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="245.0" styleClass="btn-delete" text="Delete account">
